{
    "manifest_version": 3,
    "name": "Vella",
    "description": "Vella",
    "version": "1.0.1",
    "content_scripts": [
      {
        "all_frames": true,
        "matches": [
          "<all_urls>"
        ],
        "js": [
          "js/vendor.js",
          "js/content_script.js"
        ]
      },
      {
        "matches": [
          "<all_urls>"
        ],
        "js": [
          "js/indicator.js"
        ]
      }
    ],
    "background": {
      "service_worker": "js/background.js"
    }
  }